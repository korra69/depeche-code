def my_three_method
    puts "Give me a number."
    number_1 = gets
    final_number = (((((number_1.to_i + 5) *2) - 4) / 2) - number_1.to_i)
    print final_number
end

my_three_method

def other_three_method(number_1)
    final_number = (((((number_1.to_i + 5) *2) -4) /2) - number_1.i)
    puts final_number
end

